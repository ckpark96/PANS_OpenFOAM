FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            turbulentDFSEMInlet;
        delta           2;
	nCellPerEddy    1;
	mapMethod       nearestCell;
        U           	uniform (10 0 0);
        R		uniform (0.202955 0.0851657 -0 0.328959 -0 0.218086);
        L		0.0282237;
        value		uniform (0 0 0);
    }

    outlet
    {
        type            outletInlet;
        outletValue     uniform (0 0 0);
        value           uniform (0 0 0);
    }

    upperWall
    {
        type            noSlip;
    }

    lowerWall
    {
        type            noSlip;
    }

    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
